<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="group_modulo_pc_user" model="res.groups">
        <field name="name">usuario modulo pc</field>
        <field name="category_id" ref="base.module_category_productivity"/>
    </record>

    <record id="group_modulo_pc_admin" model="res.groups">
