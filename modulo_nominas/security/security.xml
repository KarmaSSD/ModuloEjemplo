<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="group_modulo_nominas_user" model="res.groups">
        <field name="name">usuario nominas</field>
        <field name="category_id" ref="base.module_category_human_resources"/>
    </record>

    <record id="group_modulo_nominas_admin" model="res.groups">
        <field name="name">administrador nominas</field>
        <field name="category_id" ref="base.module_category_human_resources"/>
        <field name="implied_ids" eval="[(4, ref('group_modulo_nominas_user'))]"/>
    </record>
</odoo>
